#: import logcontentbox logcontentbox
#: import logface logface

<LogView>:
    orientation: 'horizontal'
    manager : app.manager

    LogFaceObjectBox:     # BoxLayout
        contentBox : log_content_box
        id : logfaceobject_box
        size_hint : (1, 1)

    LogContentBox:
        id : log_content_box
        myParent : root
        size_hint : (0.4, 1)